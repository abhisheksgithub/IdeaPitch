    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery.bootstrapvalidator/0.5.3/js/bootstrapValidator.min.js"> 
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.9/jquery.validate.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script>

        var mainText = "";
        var mainTextNumber;
        document.getElementById('sel').onchange = function () {

            var text = $(this).find('option:selected').text();
            mainText = text.trim() + "\n";
            //
            mainTextNumber = mainText.length + 50;
            //$('<p>Text</p>').update('#areaInno');
            $('#summIdea').val(mainTextNumber);
        }

        $('#summIdea').keydown(function () {
            if ($(this).val().indexOf(mainText) !== 0) {
                var length = mainText.length;
                var current = $(this).val();
                var after = current.slice(length);

                $(this).val(mainText + after);
            }
        });

        $('#summIdea').keyup(function () {
            if ($(this).val().indexOf(mainText) !== 0) {
                var length = mainText.length;
                var current = $(this).val();
                var after = current.slice(length);

                $(this).val(mainText + after);
            }
        });

        // Closes the sidebar menu
        $("#menu-close").click(function (e) {
            e.preventDefault();
            $("#sidebar-wrapper").toggleClass("active");
        });

        // Opens the sidebar menu
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#sidebar-wrapper").toggleClass("active");
        });

        // Scrolls to the selected menu item on the page
        $(function () {
            $('a[href*=#]:not([href=#])').click(function () {
                if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') || location.hostname == this.hostname) {

                    var target = $(this.hash);
                    target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
                    if (target.length) {
                        $('html,body').animate({
                            scrollTop: target.offset().top
                        }, 1000);
                        return false;
                    }
                }
            });
        });

        $(document).ready(function () {
            $('#contactForm').bootstrapValidator({
                container: '#messages',
                feedbackIcons: {
                    valid: 'glyphicon glyphicon-ok',
                    invalid: 'glyphicon glyphicon-remove',
                    validating: 'glyphicon glyphicon-refresh'
                },
                fields: {
                    honorific: {
                        validators: {
                            notEmpty: {
                                message: 'How do you want us to address you!?'+'<br/>'
                            }

                        }
                    },
                    firstName: {
                        validators: {
                            notEmpty: {
                                message: 'Really! No first name?' + '<br/>'
                            }
                        }
                    },
                    lastName: {
                        container: '#lastNamesOption',
                        validators: {
                            notEmpty: {
                                message: 'Psst.. A loophole! Put in your fav alphabet. That should let you through!' + '<br/>' 
                            }
                        }
                    },
                    email: {
                        container: '#emailAddressValidation',
                        validators: {
                            notEmpty: {
                                message: 'You need the gift voucher emailed across, dont you!? :P' + '<br/>' + '<br/>'
                            },
                            emailAddress: {
                                message: 'Nah-uh.. that mail aint right mate!' + '<br/>' + '<br/>'
                            },
                        }
                    },
                    radioButton: {
                        validators: {
                            notEmpty: {
                                message: 'This cannot be empty'
                            }
                        }
                    },
                    areaInnovation: {
                        container: '#areaOfInnovation',
                        validators: {
                            notEmpty: {
                                message: 'Think a little bit more champ, youll find your territory ' + '<br/>' + '<br/>'
                            }
                        }
                    },
                    summIdeas: {
                        container: "#summIdeaValid",
                        validators: {
                            notEmpty: {
                                message: 'Chill out, take a deep breath and i bet that the ideas will come flooding in! ' + '<br/>' + '<br/>'
                            },
                            stringLength: {
                                min: function (value, validator) {
                                    var intparse = parseInt(value,10);
                                    false;
                                },
                                message: 'Chill out, take a deep breath and i bet that the ideas will come flooding in! ' + '<br/>' + '<br/>'
                                
                            }
                        }
                    }
                }
            });
        });

        $('#loginModal').on('hidden.bs.modal', function () {
            $('#loginForm').formValidation('resetForm', true);
        });

    </script>
 
